<package>
  <name>cpmd</name>
  <executable>cpmd.x</executable>
  <charge>0</charge>
  <spin>0</spin>
  <options>none</options>
  <threads>1</threads>
  <scratch></scratch>
  <memory>1Gb</memory>
  <cleanup></cleanup>
  
  <restart>WAVEFUNCTION COORDINATES LATEST</restart>
  <optimizewf>1.0e-7</optimizewf>
  <pwcutoff>90.0</pwcutoff>
<!--   <elpot>1</elpot> -->
<!--   <projectwf>1</projectwf> -->
<!--   <basisset>6-311G-singletype</basisset> -->
<!--   <popanalysis>1</popanalysis> -->
  <getmatrices>1</getmatrices>
  <symmetry>0</symmetry>
  <cell>20.0   1.0   1.0  0.0  0.0  0.0</cell>
  
  <functional>
    <name>HCTH</name>
    <pseudopotentials help="Pseudopotential file names for each atom and their read options.">
      <O>O_MT_HCTH.psp KLEINMAN-BYLANDER</O>
      <H>H_CVB_HCTH.psp</H>
    </pseudopotentials>
    <l help="Data about angular momentum of orbitals in each pseudopotential file.">
      <O>LMAX=P</O>
      <H>LMAX=S</H>
    </l>
    
  </functional>
  
</package>
